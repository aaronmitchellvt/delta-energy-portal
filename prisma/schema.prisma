// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(cuid())
  email    String @unique
  password String
}

model Property {
  id                   String @id @default(cuid())
  propAddress          String
  propClientName       String
  propClientPhone      String
  propSqFt             Int
  propHeight           Int
  propNumOccupants     Int
  propNumWindows       Int
  propNumExteriorDoors Int
}
